{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:D-vision/iDoctor.kz-LVG.git"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "alexusmai/ruslug": "~0.6",
        "barryvdh/laravel-ide-helper": "^2.4",
        "bensampo/laravel-enum": "^1.4",
        "davejamesmiller/laravel-breadcrumbs": "4.x",
        "doctrine/dbal": "^2.5",
        "intervention/image": "^2.4",
        "irazasyed/telegram-bot-sdk": "^2.2",
        "ixudra/curl": "6.*",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.3.0",
        "maatwebsite/excel": "^2.1",
        "nao-pon/flysystem-google-drive": "~1.1",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^0.9.2",
        "wapmorgan/morphos": "^3.2",
        "idoctor/lvg":"master@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helpers.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
